{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "required": [
    "status",
    "codigo",
    "erros"
  ],
  "properties": {
    "status": {
      "type": "string"
    },
    "codigo": {
      "type": "integer"
    },
    "erros": {
      "type": "object",
      "required": [
        "erro"
      ],
      "properties": {
        "erro": {
          "type": "string"
        }
      }
    }
  }
}